<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dsdm.yuuuuunmi.calculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Calculator1Activity"
            android:label="Calculator 01">
            <intent-filter>
                <action android:name="com.dsdm.yuuuuunmi.calculator.cal"></action>
                <category android:name="android.intent.category.DEFAULT"></category>

            </intent-filter>
        </activity>
        <activity
            android:name=".Calculator2Activity"
            android:label="Calculator 02">
            <intent-filter>
                <action android:name="com.dsdm.yuuuuunmi.calculator.cal"></action>
                <category android:name="android.intent.category.DEFAULT"></category>

            </intent-filter>
        </activity>

    </application>

</manifest>